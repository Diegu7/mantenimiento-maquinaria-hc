<div class="form-group">
  <%= f.label :name, "Nombre" %>
  <%= f.text_field :name, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :tipe, "Categoría" %>
  <%= f.select :tipe, ["Categoría 1", "Categoría 2", "Categoría 3"], {}, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :brand, "Marca" %>
  <%= f.select :brand, ["Marca 1", "Marca 2", "Marca 3"], {}, class: "form-control" %>
</div>
